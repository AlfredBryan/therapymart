{
  "expo": {
    "name": "TherapyMart",
    "description": "This is a Therapy App to connect Therapist and patient",
    "slug": "TherapyMart",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/app-icon.png",
    "splash": {
      "image": "./src/images/launch.png",
      "resizeMode": "contain",
      "backgroundColor": "#01ADBA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
